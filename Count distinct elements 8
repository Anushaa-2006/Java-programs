import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
int size = sc.nextInt();
        HashSet<Integer> set = new HashSet<>();
        for (int i = 0; i < size; i++) {
            int num = sc.nextInt();
            set.add(num);   }
System.out.println("There are " + set.size() + " distinct element" +
            (set.size() > 1 ? "s" : "") + " in the array.");
    } }
